<p id="notice"><%= notice %></p>

<h1>Cert Agencies</h1>

<table>
  <thead>
    <tr>
      <th>Cert agency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cert_agencies.each do |cert_agency| %>
      <tr>
        <td><%= cert_agency.cert_agency %></td>
        <td><%= link_to 'Show', cert_agency %></td>
        <td><%= link_to 'Edit', edit_cert_agency_path(cert_agency) %></td>
        <td><%= link_to 'Destroy', cert_agency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cert Agency', new_cert_agency_path %>
